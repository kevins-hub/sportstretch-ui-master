{
  "cli": {
    "version": ">= 9.0.3"
  },
  "build": {
    "testflight": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "development": {
      "env": {
        "API_URL": "https://sportstretch-api.herokuapp.com/",
        "ENVIRONMENT": "qa"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "development-simulator": {
      "env": {
        "API_URL": "https://sportstretch-api.herokuapp.com/",
        "ENVIRONMENT": "qa"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production-beta": {
      "distribution": "internal",
      "env": {
        "API_URL": "https://sportstretch-prod.herokuapp.com/",
        "ENVIRONMENT": "prod"
      }
    }
  },
  "submit": {
    "testflight": {
      "ios": {
        "appleId": "sportstretchapp@gmail.com",
        "ascAppId": "6624313855",
        "appleTeamId": "NLY6C5D66U"
      }
    },
    "production": {
      "ios": {
        "appleId": "sportstretchapp@gmail.com",
        "ascAppId": "6624313855",
        "appleTeamId": "NLY6C5D66U"
      }
    }
  }
}
